{
  "expo": {
    "name": "clickSOS",
    "slug": "clickSOS",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "clicksos",
    "icon": "./assets/imgs/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/imgs/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/imgs/adaptive-icon.png",
        "backgroundColor": "#fdf0d5"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.clickSOS.app"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/imgs/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d73bd2d8-02d0-4b34-a664-510b7234b516"
      }
    }
  }
}
